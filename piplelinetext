pipeline {
    agent any

    stages {
        stage('mvn clean install') {
            steps {
                echo 'Building..'
            }
        }
        stage('mvn Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
} 
 
